        <xs:element ref="outer"/>
        <xs:element name="core">
          <xs:complexType/>
        </xs:element>
        <xs:element ref="NK"/>
        <xs:element ref="NA"/>
        <xs:element ref="other"/>
      </xs:choice>
    </xs:complexType>
