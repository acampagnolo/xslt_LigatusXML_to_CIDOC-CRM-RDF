    <xs:complexType/>
  </xs:element>
  <xs:element name="resewn">
    <xs:complexType/>
  </xs:element>
  <xs:element name="allAlong">
    <xs:complexType/>
  </xs:element>
  <xs:element name="multipleSectionSewing">
    <xs:complexType/>
  </xs:element>
